<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team7.wakeuptaroapp" android:versionCode="@integer/version_code" android:versionName="@string/version_name" >

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name="com.team7.wakeuptaroapp.activity.StartActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.team7.wakeuptaroapp.activity.AlarmListActivity"
            android:label="@string/title_activity_alarm_list" >
        </activity>
    </application>

</manifest>
